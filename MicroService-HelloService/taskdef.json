{
  "family": "helloserviceTD",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::382828593676:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hello-container",
      "image": "382828593676.dkr.ecr.us-east-1.amazonaws.com/service1:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
